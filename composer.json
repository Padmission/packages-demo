{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "fakerphp/faker": "^1.24",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "filament/spatie-laravel-settings-plugin": "^4.0",
        "filament/spatie-laravel-tags-plugin": "^4.0",
        "flowframe/laravel-trend": "^0.4.0",
        "guzzlehttp/guzzle": "^7.2",
        "lara-zeus/spatie-translatable": "^1.0@alpha",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.21",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "padmission/data-lens": "*",
        "phiki/phiki": "^1.1",
        "relaticle/custom-fields": "^2.0",
        "squirephp/countries-en": "^3.3",
        "archilex/filament-filter-sets": "^4.0",
        "squirephp/currencies-en": "^3.3"
    },
    "require-dev": {
        "filament/blueprint": "^1.0",
        "larastan/larastan": "^3.1",
        "laravel/boost": "^1.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-browser": "^4.0",
        "pestphp/pest-plugin-drift": "^4.0",
        "phpstan/phpstan-deprecation-rules": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "PackagesDemo\\Documentation\\": "app-modules/Documentation/src",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "cs": [
            "pint"
        ],
        "pint": "pint",
        "pest": "pest",
        "test:phpstan": "phpstan analyse",
        "test": [
            "@test:phpstan"
        ],
        "switch:3x": "./scripts/switch-branch.sh 3.x",
        "switch:4x": "./scripts/switch-branch.sh 4.x"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "name": "filament",
            "type": "composer",
            "url": "https://packages.filamentphp.com/composer"
        },
        {
            "type": "path",
            "url": "../data-lens"
        },
        {
            "type": "composer",
            "url": "https://filament-filter-sets.composer.sh"
        }
    ]
}
